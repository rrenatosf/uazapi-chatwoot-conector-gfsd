    {
      "parameters": {
        "jsCode": "const { createClient } = require('redis');\n\nconst client = createClient({\n  socket: {\n    host: 'redis',\n    port: 6379\n  },\n  database: 10\n});\n\ntry {\n  await client.connect();\n\n  //if (!chatId) {\n    //throw new Error('N√£o foi poss√≠vel extrair o chatId do input.');\n  //}\n\n  const lockKey = $('RedisKeys').first().json.lockKey;\n  const expirationInSeconds = 36000; // 10h\n\n  // Tenta criar a trava. Retorna 'OK' se foi criada, ou null se j√° existia.\n  const lockStatus = await client.set(lockKey, 'running', {\n    NX: true,\n    EX: expirationInSeconds\n  });\n\n  $input.item.json.lockStatus = lockStatus;\n\n} catch (error) {\n  throw error;\n} finally {\n  if (client.isOpen) {\n    await client.quit();\n  }\n}\n\nreturn $input.item;"
      },
